<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

	
	<bean class="com.java.spring.student.Student" id="stdId">
		<property name="rollno" value="123" />
		<property name="name" value="Alex Carry" />
		<property name="email" value="alex@gamil" />
		<property name="course" value="B.Tech" />
	</bean>
	
	<bean class="com.java.spring.student.Student" id="stdId2">
		<property name="rollno" value="12" />
		<property name="name" value="King Martin" />
		<property name="email" value="king@gamil" />
		<property name="course" value="MBA" />
	</bean>
	
</beans>